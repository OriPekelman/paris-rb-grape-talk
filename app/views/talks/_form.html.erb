<%= simple_form_for(@talk) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :proposed_on %>
    <%= f.input :meetup_id %>
    <%= f.input :user_id %>
    <%= f.input :level %>
    <%= f.input :duration %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
