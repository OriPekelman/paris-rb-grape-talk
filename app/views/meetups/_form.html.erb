<%= simple_form_for(@meetup) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :location_id %>
    <%= f.input :scheduled %>
    <%= f.input :start_hour %>
    <%= f.input :end_hour %>
    <%= f.input :meetup_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
